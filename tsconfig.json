{
    "compilerOptions": {
        "baseUrl": ".",
        "module": "ESNext",
        "target": "ES6",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "importHelpers": true,
        "isolatedModules": false,
        "strictNullChecks": true,
        "typeRoots": [
            ".dev",
            ".dev/types"
        ],
        "paths": {
            "templater-obsidian": [".dev/node_modules/templater-obsidian"],
            "templater-obsidian/*": [".dev/node_modules/templater-obsidian/*"]
        },
        "types": ["obsidian"],
        "lib": [
            "DOM",
            "ES5",
            "ES6",
            "ES7"
        ],
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        ".dev/**/*.ts",
        "*.js",
        "**/*.js",
        "*.ts",
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}